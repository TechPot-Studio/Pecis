<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type="text/xsl" href="func.xslt" ?>
<func>
    <name>isNumeric()</name>
    <returns>boolean</returns>
    <description>
        Determine whether a data is numeric
    </description>
    <usages>
        <usage>
            <definition>isNumeric( obj )</definition>
            <avaliable>0.0.1</avaliable>
            <params>
                <param>
                    <name>obj</name>
                    <type>number | string</type>
                    <optional>no</optional>
                    <description>
                        The string or number to operate
                    </description>
                </param>
            </params>
        </usage>
    </usages>

    <shorthands>
        <shorthand>
            none
        </shorthand>
    </shorthands>
</func>
