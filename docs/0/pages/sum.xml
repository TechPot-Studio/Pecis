<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type="text/xsl" href="func.xslt" ?>
<func>
    <name>sum()</name>
    <returns>number</returns>
    <description>
        Sum the numbers
    </description>
    <usages>
        <usage>
            <definition>sum( ...numbers )</definition>
            <avaliable>0.0.1</avaliable>
            <params>
                <param>
                    <name>numbers</name>
                    <type>number</type>
                    <optional>no</optional>
                    <description>
                        The numbers to sum
                    </description>
                </param>
            </params>
        </usage>
        <usage>
            <definition>sum( numbers )</definition>
            <avaliable>0.0.1</avaliable>
            <params>
                <param>
                    <name>numbers</name>
                    <type>Array&lt;number&gt;</type>
                    <optional>no</optional>
                    <description>
                        An array of numbers to sum
                    </description>
                </param>
            </params>
        </usage>
    </usages>

    <shorthands>
        <shorthand>
            none
        </shorthand>
    </shorthands>
</func>
