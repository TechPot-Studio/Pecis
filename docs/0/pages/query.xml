<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type="text/xsl" href="func.xslt" ?>
<func>
    <name>query()</name>
    <returns>ElementManager</returns>
    <description>
        Select a HTMLElement and manage it
    </description>
    <usages>
        <usage>
            <definition>query( selector )</definition>
            <avaliable>0.0.1</avaliable>
            <params>
                <param>
                    <name>selector</name>
                    <type>string</type>
                    <optional>no</optional>
                    <description>
                        The Query Selector of the element
                    </description>
                </param>
            </params>
        </usage>
    </usages>

    <shorthands>
        <shorthand>
            none
        </shorthand>
    </shorthands>
</func>
