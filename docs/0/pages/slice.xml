<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type="text/xsl" href="func.xslt" ?>
<func>
    <name>slice()</name>
    <returns>any</returns>
    <description>
        Call slice on an object
    </description>
    <usages>
        <usage>
            <definition>slice( obj, start, end )</definition>
            <avaliable>0.0.1</avaliable>
            <params>
                <param>
                    <name>obj</name>
                    <type>string | number</type>
                    <optional>no</optional>
                    <description>
                        The object to slice
                    </description>
                </param>
            </params>
        </usage>
    </usages>

    <shorthands>
        <shorthand>
            none
        </shorthand>
    </shorthands>
</func>
